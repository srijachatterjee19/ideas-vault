generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Idea {
  id         Int      @id @default(autoincrement())
  title      String
  note       String
  tags       Json     @default("[]")
  created_at DateTime @default(now())

  @@index([created_at])
}
